[ignore]
.*/node_modules/.*

[include]

[libs]
flow-typed

[declarations]
.*/node_modules/.*

[lints]
all=error

[options]
all=true
babel_loose_array_spread=true
emoji=true
esproposal.export_star_as=enable
esproposal.nullish_coalescing=enable
esproposal.optional_chaining=enable
exact_by_default=true
module.ignore_non_literal_requires=true

[strict]
