<% commands.forEach((command) => { -%>
.TH "<%= command.command %>" 1 "<%= new Intl.DateTimeFormat().format(new Date()) %>"
.SH NAME
<%= command.command %>
.SH DESCRIPTION
.B <%= command.command %>
<%= command.description %>
<% if (Object.keys(command.positionals).length) { -%>
.SH POSITIONALS
<% Object.keys(command.positionals).forEach((key) => { %>
\-\-<%= key %> \- <%= command.positionals[key].description %>
<% });
}%>
<% if (Object.keys(command.options).length) { -%>
.SH OPTIONS
<% Object.keys(command.options).forEach((key) => { %>
<% const option = command.options[key] -%>
.IP <% if (option.type in ['array', 'string', 'number']) { %>"--<%= key %> <%= key %>"<% } else { %>--<%= key %><% } %>
<%= command.options[key].description %>
<% });
}%>
<% if (command.examples.length) { -%>
.SH EXAMPLES
<% command.examples.forEach(({ code, description }) => { -%>
<%= description %>

.RS
<%= code %>
.RE
<% }); -%>
<% } -%>
<% }); -%>
